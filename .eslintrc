{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "env": {
      "browser": false,
      "node": true,
      "jest": true,
    },
    "rules": {
      "func-names": 0,
      "strict": 0,
      "newline-per-chained-call": 0,
      "comma-dangle": ["error", {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "functions": "ignore"
      }],
      "no-underscore-dangle": 0,
      "max-len": ["error", 120],
      "no-use-before-define": ["error", { "functions": false }]
    }
}
